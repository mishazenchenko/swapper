idate } from '@vuelidate/core';\nimport { useI18n } from 'vue-i18n';\n\nexport default {\n  components: { StatusIcon },\n  mixins: [VuelidateMixin],\n  setup() {\n    return {\n      v$: useVuelidate(),\n    };\n  },\n  data() {\n    return {\n      $t: useI18n().t,\n      confirmed: false,\n    };\n  },\n  validations: {\n    confirmed: {\n      mustBeTrue: (value) => value === true,\n    },\n  },\n  methods: {\n    closeModal() {\n      this.$nextTick(() => {\n        this.$refs.modal.hide();\n      });\n    },\n    handleSubmit() {\n      this.v$.$touch();\n      if (this.v$.$invalid) return;\n      this.$emit('ok');\n      this.closeModal();\n    },\n    resetForm() {\n      this.confirmed = false;\n      this.v$.$reset();\n    },\n  },\n};\n</script>\n"]}]}