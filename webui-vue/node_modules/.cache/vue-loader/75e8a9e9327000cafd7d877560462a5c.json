nents: { IconView, IconViewOff },\n  data() {\n    return {\n      isVisible: false,\n      togglePasswordLabel: i18n.global.t('global.ariaLabel.showPassword'),\n    };\n  },\n  methods: {\n    toggleVisibility() {\n      const firstChild = this.$children[0];\n      const inputEl = firstChild ? firstChild.$el : null;\n\n      this.isVisible = !this.isVisible;\n\n      if (inputEl && inputEl.nodeName === 'INPUT') {\n        inputEl.type = this.isVisible ? 'text' : 'password';\n      }\n\n      this.isVisible\n        ? (this.togglePasswordLabel = i18n.global.t(\n            'global.ariaLabel.hidePassword',\n          ))\n        : (this.togglePasswordLabel = i18n.global.t(\n            'global.ariaLabel.showPassword',\n          ));\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import '@/assets/styles/bmc/helpers/_index.scss';\n@import '@/assets/styles/bootstrap/_helpers.scss';\n\n.input-password-toggle-container {\n  position: relative;\n}\n</style>\n"]}]}