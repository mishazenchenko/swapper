{"ast":null,"code":"import UpToTop24 from '@carbon/icons-vue/es/up-to-top/24';\nimport { debounce } from 'lodash';\nimport { useI18n } from 'vue-i18n';\nexport default {\n  name: 'BackToTop',\n  components: {\n    IconUpToTop: UpToTop24\n  },\n  data() {\n    return {\n      $t: useI18n().t,\n      showButton: false\n    };\n  },\n  created() {\n    window.addEventListener('scroll', debounce(this.handleScroll, 200));\n  },\n  methods: {\n    handleScroll() {\n      document.documentElement.scrollTop > 500 ? this.showButton = true : this.showButton = false;\n    },\n    scrollToTop() {\n      document.documentElement.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    }\n  }\n};","map":{"version":3,"names":["UpToTop24","debounce","useI18n","name","components","IconUpToTop","data","$t","t","showButton","created","window","addEventListener","handleScroll","methods","document","documentElement","scrollTop","scrollToTop","scrollTo","top","behavior"],"sources":["/home/zenchenko/openbmc/build/evb-ast2500/workspace/sources/webui-vue/src/components/Global/ButtonBackToTop.vue"],"sourcesContent":["<template>\n  <b-button\n    id=\"scrollToTopBtn\"\n    class=\"btn-top btn-icon-only\"\n    :class=\"{ 'show-btn': showButton }\"\n    variant=\"secondary\"\n    :title=\"$t('global.ariaLabel.scrollToTop')\"\n    @click=\"scrollToTop\"\n  >\n    <icon-up-to-top />\n    <span class=\"sr-only\">{{ $t('global.ariaLabel.scrollToTop') }}</span>\n  </b-button>\n</template>\n\n<script>\nimport UpToTop24 from '@carbon/icons-vue/es/up-to-top/24';\n\nimport { debounce } from 'lodash';\nimport { useI18n } from 'vue-i18n';\n\nexport default {\n  name: 'BackToTop',\n  components: { IconUpToTop: UpToTop24 },\n  data() {\n    return {\n      $t: useI18n().t,\n      showButton: false,\n    };\n  },\n  created() {\n    window.addEventListener('scroll', debounce(this.handleScroll, 200));\n  },\n  methods: {\n    handleScroll() {\n      document.documentElement.scrollTop > 500\n        ? (this.showButton = true)\n        : (this.showButton = false);\n    },\n    scrollToTop() {\n      document.documentElement.scrollTo({\n        top: 0,\n        behavior: 'smooth',\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import '@/assets/styles/bmc/helpers/_index.scss';\n@import '@/assets/styles/bootstrap/_helpers.scss';\n\n.btn-top {\n  position: fixed;\n  bottom: 24px;\n  right: 24px;\n\n  box-shadow: $box-shadow;\n  visibility: hidden;\n  opacity: 0;\n  transition: $transition-base;\n  z-index: $zindex-fixed;\n\n  @media (min-width: 1600px) {\n    left: 1485px;\n    right: auto;\n  }\n}\n.show-btn {\n  visibility: visible;\n  opacity: 1;\n}\n</style>\n"],"mappings":"AAeA,OAAOA,SAAQ,MAAO,mCAAmC;AAEzD,SAASC,QAAO,QAAS,QAAQ;AACjC,SAASC,OAAM,QAAS,UAAU;AAElC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAEC,WAAW,EAAEL;EAAU,CAAC;EACtCM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,EAAE,EAAEL,OAAO,CAAC,CAAC,CAACM,CAAC;MACfC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEX,QAAQ,CAAC,IAAI,CAACY,YAAY,EAAE,GAAG,CAAC,CAAC;EACrE,CAAC;EACDC,OAAO,EAAE;IACPD,YAAYA,CAAA,EAAG;MACbE,QAAQ,CAACC,eAAe,CAACC,SAAQ,GAAI,GAAE,GAClC,IAAI,CAACR,UAAS,GAAI,IAAI,GACtB,IAAI,CAACA,UAAS,GAAI,KAAM;IAC/B,CAAC;IACDS,WAAWA,CAAA,EAAG;MACZH,QAAQ,CAACC,eAAe,CAACG,QAAQ,CAAC;QAChCC,GAAG,EAAE,CAAC;QACNC,QAAQ,EAAE;MACZ,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}